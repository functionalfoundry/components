{
  "name": "@workflo/components",
  "version": "0.5.0",
  "description": "Reusable themable components from Workflo",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 2000",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workflo/components.git"
  },
  "author": "Yaniv Tal",
  "license": "Copyright Workflo",
  "bugs": {
    "url": "https://github.com/workflo/components/issues"
  },
  "homepage": "https://github.com/workflo/components#readme",
  "dependencies": {
    "@workflo/styles": "^0.5.0",
    "js-theme": "^0.4.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.6.1",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "commitizen": "^2.8.6",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.11.4",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.1.2",
    "jest-cli": "^15.1.1",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "semantic-release": "^4.3.5",
    "webpack": "^1.13.2"
  },
  "peerDependencies": {
    "react": "15.3.1",
    "react-dom": "15.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/!(*.story)*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
